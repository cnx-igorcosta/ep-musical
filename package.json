{
  "name": "ep-musical",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node ./dist/app.js",
    "start:dev": "nodemon ./app/app.js --exec babel-node",
    "create-source": "rimraf kinghost [\"ENOTEMPTY\"]",
    "copy-files": "cpx \"app/public/**\" \"kinghost/public/\" && cpx \"package.json\" \"kinghost/\" && cpx \"package-lock.json\" \"kinghost/\"",
    "transpile-code": "babel app -d kinghost --ignore app/public",
    "install-dependencies": "cd ../apps_nodejs && npm install",
    "package": "npm run create-source && npm run copy-files && npm run transpile-code",
    "build": "rimraf ../apps_nodejs [\"ENOTEMPTY\"] && cpx \"kinghost/**\" \"../apps_nodejs/\" && npm run install-dependencies"
  },
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "body-parser": "~1.17.1",
    "cookie-parser": "~1.4.3",
    "cpx": "^1.5.0",
    "debug": "~2.6.3",
    "ejs": "^2.5.7",
    "express": "~4.15.2",
    "express-myconnection": "^1.0.4",
    "jade": "~1.11.0",
    "moment": "^2.18.1",
    "morgan": "~1.8.1",
    "mysql": "^2.13.0",
    "png-to-jpeg": "^1.0.1",
    "renamer": "^0.6.1",
    "rimraf": "^2.6.1",
    "serve-favicon": "~2.4.2"
  },
  "devDependencies": {
    "nodemon": "^1.11.0"
  }
}
