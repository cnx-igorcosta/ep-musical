{
  "name": "ep-musical",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node ./dist/src/bin/www",
    "start:dev": "node_modules/.bin/nodemon ./app/src/bin/www --exec babel-node",
    "start:prd": "npm run build && npm start",
    "build": "rimraf dist [\"ENOTEMPTY\"] && babel app -d dist --ignore app/public && npm run copy-files",
    "copy-files": "cpx \"app/src/bin/*\" \"dist/src/bin/\" && cpx \"app/public/**\" \"dist/public/\"",
    "create-source:kinghost": "rimraf ../apps_nodejs [\"ENOTEMPTY\"]",
    "transpile-code:kinghost": "babel app -d ../apps_nodejs --ignore app/public",
    "copy-files:kinghost": "cpx \"app/src/bin/*\" \"../apps_nodejs/src/bin/\" && cpx \"app/public/**\" \"../apps_nodejs/public/\"  && cpx \"node_modules/**\" \"../apps_nodejs/node_modules/\"",
    "deploy:kinghost": "npm run create-source:kinghost && npm run transpile-code:kinghost && npm run copy-files:kinghost",
    "start:kinghost": "node ../apps_nodejs/app.js"


  },
  "dependencies": {
    "babel-core": "^6.25.0",
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "body-parser": "~1.17.1",
    "cookie-parser": "~1.4.3",
    "cpx": "^1.5.0",
    "debug": "~2.6.3",
    "ejs": "^2.5.7",
    "express": "~4.15.2",
    "express-myconnection": "^1.0.4",
    "jade": "~1.11.0",
    "morgan": "~1.8.1",
    "mysql": "^2.13.0",
    "rimraf": "^2.6.1",
    "serve-favicon": "~2.4.2"
  },
  "devDependencies": {
    "nodemon": "^1.11.0"
  }
}
